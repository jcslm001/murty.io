{
  "name": "murty",
  "version": "3.5.0",
  "author": "Brendan Murty <b@murty.io> (http://b.murty.io)",
  "homepage": "http://murty.io",
  "description": "The Murty website built with Angular and Express, coded with Atom and hosted by Digital Ocean.",
  "private": true,
  "bugs": {
    "url": "https://github.com/brendanmurty/website/issues/new?assignee=brendanmurty"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brendanmurty/website"
  },
  "license": "SEE LICENSE IN license.md",
  "dependencies": {
    "express": "^4.15.3"
  },
  "devDependencies": {
    "bower": "^1.7.7",
    "compression": "^1.6.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-uglify": "^2.1.2",
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "postinstall": "bower install --allow-root",
    "startdev": "nodemon server.js",
    "start": "set NODE_ENV=production && export APP_DOMAIN=murty.io && export APP_PORT=80 && forever start -o murty-output.log -e murty-error.log -l murty-forever.log -a server.js",
    "stop": "forever stopall",
    "logserver": "tail -f ~/.forever/murty-forever.log",
    "logoutput": "tail -f ~/.forever/murty-output.log",
    "logerror": "tail -f ~/.forever/murty-error.log"
  }
}
